<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h4>Jobs Available</h4>       
          <% @posts.each do |post| %>
            <div class="jumbotron jumbotron-special">
              <p><%= link_to post.title, "/posts/#{post.id}"%></p>
              <p><%= raw(post.description) %></p>
            </div>
            <br>
          <% end %>
    </div>
    
    <div class="col-md-4">
    </div>
  </div>
</div>